@model EParking.Models.Clan

@{
    ViewData["Title"] = "Delete";
}



    <div class="landing-section">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-8">
                    <div class="card">
                        <div class="card-header">Brisanje</div>
                        <div class="card-body">

                            <form asp-action="Obrada">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div class="row justify-content-left">
                                    <h5>Rezervisano parking mjesto: @Model.RezervisanoParkingMjesto</h5>
                                </div>
                                <div class="row justify-content-left">
                                    <h5>Status članarine: @Model.StatusClanarine</h5>
                                </div>
                                <div class="row justify-content-left">
                                    <h5>Tip članarine: @Model.TipClanarine</h5>
                                </div>
                                <div class="row justify-content-left">
                                    <h5>Ime i prezime: @Model.ImePrezime</h5>
                                </div>
                                <div class="row justify-content-left">
                                    <h5>Username: @Model.Username</h5>
                                </div>
                                <div class="row justify-content-left">
                                    <h5>JMBG: @Model.JMBG</h5>
                                </div>
                                <div class="row justify-content-left">
                                    <h5>Adresa: @Model.Adresa</h5>
                                </div>
                                <div class="row justify-content-left">
                                    <h5>Email: @Model.Email</h5>
                                </div>
                                <div class="row justify-content-left">
                                    <h5>Broj telefona: @Model.BrojMobitela</h5>
                                </div>

                                <br />
                                <br />

                            </form>
                        </div>

                        <form asp-action="Delete">
                            <div class="row justify-content-center">
                                <input type="hidden" asp-for="ID" />
                                <input type="submit" value="Obriši" class="btn btn-danger" /> &nbsp; &nbsp; | &nbsp; &nbsp;
                                <a asp-action="Index">Nazad</a>
                            </div>

                        </form>
                    </div>

                </div>
            </div>


        </div>
</div>