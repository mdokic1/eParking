@model IEnumerable<EParking.Models.Clan>

@{
    ViewData["Title"] = "Index";
}



    <div class="landing-section">

        <h1>Član</h1>
        <p>
            <a asp-controller="Administrator" asp-action="Account">Nazad na početnu</a>
        </p>

        <table class="table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.RezervisanoParkingMjesto)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.StatusClanarine)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TipClanarine)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ImePrezime)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Username)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Password)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.JMBG)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Adresa)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.BrojMobitela)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Email)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.RezervisanoParkingMjesto)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.StatusClanarine)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TipClanarine)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ImePrezime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Username)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Password)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.JMBG)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Adresa)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.BrojMobitela)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            <a asp-action="Delete" asp-route-id="@item.ID">Obriši</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
